{
    "name": "KevinSpace-y",
    "description": "Delete Pinned Tweets",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
      "service_worker": "service_worker.js",
      "persistent": false
    },
    "icons": {
      "16": "images/favicon16.png",
      "32": "images/favicon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "action": {
      "default_icon": {
        "16": "images/favicon16.png",
        "32": "images/favicon32.png"
      }
    },
    "permissions": [
      "activeTab",
      "webRequest",
      "storage"
    ],
    "host_permissions": [
      "https://*.twitter.com/*"
    ],
    "content_scripts": [
      {
        "matches": [
          "https://*.twitter.com/*"
        ],
        "js": [
          "contentScript.js"
        ]
      }
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "images/*.png"
        ],
        "extension_ids": [
          "kmmakimpficddgkkcdhokaagahodnhpj"
        ]
      }
    ]
  }